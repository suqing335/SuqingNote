+		len = fseekNunlines(BOOT_FD);
+		if (len <= 0) {
+		   	printf("Error: file is not of the correct format...\n");
+		   	return -1;
+		}
+		printf("fileLength = %u\n", len);
+        /*start at the beginning of the file*/
+        //fseek(BOOT_FD, 0, SEEK_SET);
+
